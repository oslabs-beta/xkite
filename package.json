{
  "name": "xkite",
  "version": "0.1.0",
  "description": "XKite is a web-based tool for creating, managing, and analyzing Kafka clusters. It is built on top of the Kafkajs library and uses Docker to create and manage the Kafka cluster.",
  "author": "XKite Team- OpenSource Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oslabs-beta/xkite"
  },
  "scripts": {
    "spring": "mvn -f ./src/common/springApp/pom.xml spring-boot:run",
    "build-spring": "mvn -f ./src/common/springApp clean package && mv ./src/common/springApp/target/*.jar ./src/common/kite/download/spring/app.jar",
    "next-dev": "next dev",
    "dev:6662": "next dev -p 6662",
    "dev": "concurrently \"open http://localhost:3000\" \"npm run dev:6662\" \"next dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "bin": {
    "xkite": "bin/cli"
  },
  "keywords": [
    "apache",
    "kafka",
    "kafkajs",
    "docker",
    "react",
    "nextjs"
  ],
  "dependencies": {
    "@next/font": "13.0.6",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/node": "18.14.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "axios": "^1.3.4",
    "bootstrap": "^5.2.3",
    "concurrently": "^7.6.0",
    "eslint": "8.34.0",
    "eslint-config-next": "13.0.6",
    "next": "13.0.6",
    "react": "18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.2.7",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/js-yaml": "^4.0.5",
    "@types/kafkajs": "^1.9.0",
    "@types/react-bootstrap": "^0.32.32",
    "child_process": "^1.0.2",
    "docker-compose": "^0.23.19",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^11.1.0",
    "js-yaml": "^4.1.0",
    "kafkajs": "^2.2.3",
    "prettier": "^2.8.4",
    "prettier-eslint": "^15.0.1",
    "sass": "^1.58.3",
    "zip-local": "^0.3.5"
  },
  "browser": {
    "child_process": false,
    "fs": false
  }
}
